#include <stdio.h>
#include <stdlib.h>


main(){

    int d, m;

    printf("Informe o dia e mes [dd/mm]: \n");
    scanf("%d/%d", &d, &m);

    if((d<0 || d>31) && (m<0 || m>12))
        printf("\nERRO\n");

    else if((m==12 && d>=21) || (m<=3 && d<=20))

        printf("\nVERÃO\n");

    else if((m>=3 && d>=21) || (m<=6 && d<=20)) //ta tendo algum erro ai, que quando coloca 21/06 imprimi outono ainda e não inverno

        printf("\nOUTONO\n");

    else if((m>=6 && d>=21) || (m<=9 && d<= 20))

        printf("\nINVERNO\n");

    else

        printf("\nPRIMAVERA\n");



system("PAUSE");
}
